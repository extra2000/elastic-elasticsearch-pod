cluster.name: elk-cluster-01
node.name: es-master
node.roles: [master, remote_cluster_client, data_hot, data_content, ingest, ml, transform]
network.host: es-master-01.mydomain
http.port: 9200
transport.port: 9300
discovery.type: single-node
bootstrap.memory_lock: false
indices.fielddata.cache.size: 20%
xpack.license.self_generated.type: basic
xpack.security.enabled: true
xpack.security.http.ssl.enabled: true
xpack.security.http.ssl.certificate_authorities: [elastic-ca.crt]
xpack.security.http.ssl.certificate: es-master-http.crt
xpack.security.http.ssl.key: es-master-http.key
xpack.security.http.ssl.verification_mode: full
xpack.security.transport.ssl.enabled: true
xpack.security.transport.ssl.verification_mode: full
xpack.security.transport.ssl.certificate_authorities: [elastic-ca.crt]
xpack.security.transport.ssl.certificate: es-master-transport.crt
xpack.security.transport.ssl.key: es-master-transport.key
xpack.security.transport.ssl.client_authentication: required
s3.client.default.protocol: https
s3.client.default.endpoint: "minio.mydomain:9000"
s3.client.default.path_style_access: "true"
reindex.remote.whitelist: []
cluster.routing.allocation.disk.watermark.enable_for_single_data_node: true
